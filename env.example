# Environment Variables for Team Collaboration App Frontend
# Copy this file to .env.local for local development
# Set these variables in Vercel dashboard for production

# FastAPI Backend URL (your deployed Render URL)
REACT_APP_API_BASE_URL=https://your-backend-app.onrender.com

# WebSocket Server URL (will be set after Node.js deployment)
REACT_APP_WS_URL=https://your-websocket-app.railway.app

# Optional: Enable debug mode in production (set to false for production)
REACT_APP_DEBUG=false
